<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daini.calculadoraimc">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CalculadoraActivity" />
        <activity android:name=".Activity.HistorialActivity" />
        <activity android:name=".Activity.ResultadosActivity" />
        <activity android:name=".Activity.RangosActivity" />
        <activity android:name=".Activity.RegistroActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.WebViewActivity"></activity>
    </application>

</manifest>